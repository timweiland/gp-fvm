all:
	@echo "Running all"

advection-data:
	@echo "Downloading 1D Advection data from PDEBench..."
	@wget -O ./data/1D_Advection_Sols_beta0.4.hdf5 https://darus.uni-stuttgart.de/api/access/datafile/255674

darcy-data:
	@echo "Downloading 2D Darcy Flow data from PDEBench..."
	@wget -O ./data/2D_DarcyFlow_beta1.0.hdf5 https://darus.uni-stuttgart.de/api/access/datafile/133219

